{
    "name": "phs/flowers",
    "description": "The Flower Show App.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
      {
        "type": "composer",
        "url": "https://nova.laravel.com"
      }
    ],
    "require": {
        "php": "^8.2",
        "akki-io/laravel-nova-search": "^1.0",
        "aws/aws-sdk-php": "^3.228",
        "composer/composer": "dev-main",
        "doctrine/dbal": "~2.3",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/cashier": "^13.9",
        "laravel/framework": "^9.0",
        "laravel/nova": "~3.0",
        "laravel/sanctum": "^2.14",
        "laravel/telescope": "^4.7.2",
        "laravel/tinker": "^2.7",
        "laravelcollective/html": "^6.0",
        "livewire/livewire": "^2.3",
        "nztim/mailchimp": "^4.4",
        "optimistdigital/nova-inline-text-field": "^1.0",
        "optimistdigital/nova-sortable": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12.2",
        "driftingly/rector-laravel": "^0.14.1",
        "fakerphp/faker": "^1.14",
        "filp/whoops": "~2.14",
        "laravel/breeze": "^1.1",
        "laravel/sail": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "orangehill/iseed": "^3.0",
        "phpunit/phpunit": "^9.5.10",
        "rector/rector": "^0.15.2",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:eloquent",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "minimum-stability": "dev",
        "preferred-install": "dist",
        "sort-packages": true
    }
}
